{
  "root": "libs/order-list",
  "sourceRoot": "libs/order-list/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/order-list/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/order-list"],
      "options": {
        "jestConfig": "libs/order-list/jest.config.js",
        "passWithNoTests": true
      }
    },
    "generate": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx apollo client:codegen --config=libs/order-list/apollo.config.js --target=typescript --globalTypesFile=libs/order-list/src/__generated__/globalTypes.ts"
          }
        ]
      }
    }
  }
}
